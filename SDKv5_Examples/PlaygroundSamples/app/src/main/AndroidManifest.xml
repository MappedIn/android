<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlaygroundSamples"
        tools:targetApi="31"
        android:hardwareAccelerated="true"
        >

        <activity
            android:name=".MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".examples.RenderMap"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.AddInteractivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.Markers"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.ABWayfinding"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.BlueDot"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.CameraControls"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.ListLocations"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.Tooltips"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.LevelSelector"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.TurnByTurnDirections"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.Search"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.FloatingLabels"
            android:exported="false">
        </activity>
        <activity
            android:name=".examples.FlatLabels"
            android:exported="false">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <!--Permissions below are only needed if displaying user location-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT"/>


</manifest>